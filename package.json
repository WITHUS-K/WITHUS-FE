{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "web": "turbo dev --filter web",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "new-package": "hygen new package",
    "storybook": "turbo run storybook --filter @repo/ui",
    "build-storybook": "turbo run build-storybook --filter @repo/ui"
  },
  "devDependencies": {
    "hygen": "^6.2.11",
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
